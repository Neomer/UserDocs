<?xml version="1.0" encoding="utf-8" ?>
<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"	namespace="UserDocs.Models" assembly="UserDocs">
  <class name="DocumentModel" table="UserDoc">
    <id name="ID" column="ID">
      <generator class="identity"/>
    </id>
    <property name="AuthorID" column="AuthorID"  />
    <property name="Name" column="Name"  />
    <property name="CreationDate" column="CreationDate"  />
    <property name="Data" column="Data"  />
    <property name="Filename" column="Filename"  />
    <property name="ContentType" column="ContentType"  />
    <property name="Size" column="Size"  />
  </class>
</hibernate-mapping>